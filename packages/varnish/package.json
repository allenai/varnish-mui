{
  "name": "@allenai/varnish",
  "version": "0.1.1",
  "private": false,
  "author": "Reviz",
  "description": "Varnish theme and components that implement Google's Material Design.",
  "main": "./src/index.ts",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design",
    "varnish"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/allenai/varnish.git",
    "directory": "packages/varnish"
  },
  "license": "MIT",
  "homepage": "https://varnishj.allenai.org/material-ui/getting-started/overview/",
  "scripts": {
    "build": "yarn build:legacy && yarn build:modern && yarn build:node && yarn build:stable && yarn build:umd && yarn build:types && yarn build:copy-files",
    "build:umd": "cross-env BABEL_ENV=stable rollup -c scripts/rollup.config.js",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "yarn build && npm publish build",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{spec,d}.{ts,tsx}\" && tsc -p tsconfig.json",
    "typescript:module-augmentation": "node scripts/testModuleAugmentation.js"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.11.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.11.0",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@types/react-transition-group": "^4.4.5",
    "clsx": "^1.2.1",
    "csstype": "^3.1.1",
    "react-is": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "hex-rgb": "^5.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
