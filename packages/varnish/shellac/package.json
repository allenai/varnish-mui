{
  "name": "shellac",
  "version": "0.0.1",
  "private": true,
  "author": "Reviz",
  "description": "Shellac is a pared down version of Varnish that lets you make a polished, AI2 branded website with HTML alone.",
  "scripts": {
    "build": "tsc -p ./ && node ./build/varnish/shellac/src/makeShellac.js",
    "prebuild": "rimraf build",
    "postbuild": "cp ./build/shellac.css ../build/"
  },
  "devDependencies": {
    "@allenai/varnish2": "latest",
    "@mui/material": "latest",
    "@mui/system": "latest",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.3"
  },
  "resolutions": {
    "hex-rgb": "^4.3.0"
  }
}
